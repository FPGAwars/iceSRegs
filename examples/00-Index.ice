                "params": [
                  {
                    "name": "INI"
                  }
                ],
                "code": "//-- Generic Shift Left/Right\n//-- System register with load\n//-- and reset inputs\n//-- Number of bits\nlocalparam N = 2;\n\n//-- Initial value\nreg [N-1:0] q = INI;\n\nalways @(posedge clk)\nbegin\n  //-- Rst has the highest priority\n  if (rst == 1)\n  \n    //-- Load the initial value\n    q <= INI;\n\n  //-- No reset\n  else\n  \n    //- Check Load\n    if (load == 1)\n  \n      //-- load value fro din\n      q <= din;\n    \n    //-- No load\n    else\n      if (lr == 0)\n        //-- Shift to the right\n        q <= {MSB_i,q[N-1]};\n\n      //-- lr == 1    \n      else\n        //-- Shift to the left\n        q <= {q[N-2:0], lsb_i};\nend\n\n//-- Serial out: \n//-- Least significant bit\nassign lsb_o = q[0];\n\n//-- most significant bit\nassign MSB_o = q[N-1];\n"
              },
              "position": {
